{
  "name": "groupefbo/ezframe",
  "type": "library",
  "require": {
    "cbschuld/browser.php": "~1.0",
    "components/jquery": "~3.0",
    "monolog/monolog": "^2.3",
    "nadar/quill-delta-parser": "^3.0",
    "phpmailer/phpmailer": "^6.5",
    "symfony/dotenv": "^6.2",
    "symfony/var-dumper": "~5.0",
    "twbs/bootstrap": "~5.0",
    "twig/inky-extra": "^3.5",
    "twig/twig": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Core\\": [
        "cores/core/class/"
      ],
      "Core\\Common\\": [
        "cores/common/class"
      ],
      "Helper\\": [
        "helpers/",
        "../../helpers/"
      ],
      "App\\": [
        "app/",
        "../../app/"
      ]
    },
    "files": [
      "helpers/functions/func_dates.php",
      "helpers/functions/func_utils.php"
    ]
  },
  "config": {
    "optimize-autoloader": true
  },
  "license": "MIT"
}
