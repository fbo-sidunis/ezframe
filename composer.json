{
  "name": "groupefbo/ezframe",
  "type": "library",
  "require": {
    "cbschuld/browser.php": "*",
    "components/jquery": "*",
    "monolog/monolog": "*",
    "nadar/quill-delta-parser": "*",
    "phpmailer/phpmailer": "*",
    "symfony/dotenv": "*",
    "symfony/var-dumper": "*",
    "twbs/bootstrap": "*",
    "twig/inky-extra": "*",
    "twig/twig": "*"
  },
  "autoload": {
    "psr-4": {
      "Core\\": [
        "cores/core/class/"
      ],
      "Core\\Common\\": [
        "cores/common/class"
      ],
      "Helper\\": [
        "helpers/",
        "../../helpers/"
      ],
      "App\\": [
        "app/",
        "../../app/"
      ]
    },
    "files": [
      "helpers/functions/func_dates.php",
      "helpers/functions/func_utils.php"
    ]
  },
  "config": {
    "optimize-autoloader": true
  },
  "license": "MIT"
}
